<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_setting_types"
    style="@style/bg_white_style"
    android:layout_width="match_parent"
    android:layout_height="@dimen/d106">

    <TextView
        android:id="@+id/tv_alias"
        style="@style/black_16_text_style"
        android:layout_marginTop="@dimen/d20" />

    <TextView
        android:id="@+id/tv_setting_type"
        style="@style/black_14_text_style"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/tv_alias"
        android:layout_marginRight="@dimen/d5"
        android:layout_marginTop="@dimen/d16"
        android:layout_toLeftOf="@+id/ib_delete"
        android:lineSpacingExtra="@dimen/d3"
        android:maxHeight="@dimen/d44"
        android:maxLines="2" />

    <ImageButton
        android:id="@+id/ib_delete"
        style="@style/ib_style"
        android:layout_alignParentRight="true"
        android:layout_marginTop="@dimen/d12"
        android:src="@mipmap/icon_delete" />


</RelativeLayout>